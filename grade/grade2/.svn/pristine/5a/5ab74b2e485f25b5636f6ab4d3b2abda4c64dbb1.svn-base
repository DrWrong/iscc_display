<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<servlet>
		<servlet-name>AdminAction</servlet-name>
		<servlet-class>tsc.nsfocus.action.AdminAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminAction</servlet-name>
		<url-pattern>/adminAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TeamAction</servlet-name>
		<servlet-class>tsc.nsfocus.action.TeamAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TeamAction</servlet-name>
		<url-pattern>/admin/files/teamAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>NoticeAction</servlet-name>
		<servlet-class>tsc.nsfocus.action.NoticeAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NoticeAction</servlet-name>
		<url-pattern>/admin/files/noticeAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>FlagAction</servlet-name>
		<servlet-class>tsc.nsfocus.action.FlagAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FlagAction</servlet-name>
		<url-pattern>/admin/files/flagAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RewardAction</servlet-name>
		<servlet-class>tsc.nsfocus.action.RewardAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RewardAction</servlet-name>
		<url-pattern>/admin/files/rewardAction</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UserAction</servlet-name>
		<servlet-class>tsc.nsfocus.action.UserAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserAction</servlet-name>
		<url-pattern>/userAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>tsc.nsfocus.action.UserLoginAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<filter>
			<filter-name>AdminFilter</filter-name>
			<filter-class>tsc.nsfocus.util.AdminFilter</filter-class>
		<init-param>
			<param-name>url</param-name>
			<param-value>/login.jsp</param-value>
		</init-param>
	</filter>
	
	<filter-mapping>
		<filter-name>AdminFilter</filter-name>
		<url-pattern>/admin/*</url-pattern>
	</filter-mapping>
	
	<filter>
			<filter-name>UserFilter</filter-name>
			<filter-class>tsc.nsfocus.util.UserFilter</filter-class>
		<init-param>
			<param-name>url</param-name>
			<param-value>/index.jsp</param-value>
		</init-param>
	</filter>
	
	<filter-mapping>
		<filter-name>UserFilter</filter-name>
		<url-pattern>/userAction</url-pattern>
	</filter-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
