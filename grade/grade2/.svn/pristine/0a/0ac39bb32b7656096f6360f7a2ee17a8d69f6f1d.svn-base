package tsc.nsfocus.bean;

public class FlagBean {
	private Integer id;
	private String name;	//flag的md5值,或flag内容等
	private String host;
	private String value;	//flag对应的奖金，可以为空

	
	public String getHost() {
		return host;
	}
	public void setHost(String host) {
		this.host = host;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getValue() {
		return value;
	}
	public void setValue(String value) {
		this.value = value;
	}
	
	
}
